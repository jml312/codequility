generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @default(autoincrement())
  email           String   @unique
  username        String   @unique
  name            String
  profilePic      String
  description     String?
  location        String?
  yearsExperience Int?
  company         String?
  createdAt       DateTime @default(now())
}
